{
  "name": "ai-shopping-scraper",
  "version": "2.1.0",
  "description": "Focused scrapers for populating e-commerce world model with MongoDB integration",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:ts": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon src/index.js",
    "dev:ts": "nodemon --exec \"ts-node -r tsconfig-paths/register\" src/index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:system": "jest --testPathPattern=tests/system",
    "lint": "eslint src/ --fix",
    "lint:check": "eslint src/",
    "lint:ts": "eslint src/**/*.ts --fix",
    "lint:ts:check": "eslint src/**/*.ts",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "validate": "node scripts/validate-deployment.js",
    "rollback": "node scripts/rollback.js",
    "rollback:force": "node scripts/rollback.js --force",
    "docs:api": "node scripts/generate-api-docs.js",
    "docs:serve": "echo 'Open docs/api/swagger-ui.html in your browser for API documentation'",
    "pre-deploy": "npm run type-check && npm run lint:check && npm run test:coverage && npm run validate",
    "db:migrate": "node scripts/migrate.js migrate",
    "db:seed": "node scripts/seed.js",
    "scrape:test": "node test-basic.js",
    "scrape:enhanced": "node test-enhanced.js",
    "scrape:intelligence": "node test-site-intelligence.js",
    "queue:worker": "node src/workers/startWorker.js",
    "queue:worker:ts": "ts-node -r tsconfig-paths/register src/workers/startWorker.ts",
    "worker:start": "node src/workers/startWorker.js",
    "worker:start:ts": "ts-node -r tsconfig-paths/register src/workers/startWorker.ts",
    "orchestrate": "node src/scrapers/redis_product_queue_orchestrator.js",
    "orchestrate:ts": "ts-node -r tsconfig-paths/register src/scrapers/redis_product_queue_orchestrator.ts",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "metrics": "curl -s http://localhost:3000/metrics",
    "monitoring": "curl -s http://localhost:3000/monitoring/summary | jq .",
    "db:status": "node scripts/migrate.js status",
    "db:rollback": "node scripts/migrate.js rollback",
    "db:create-migration": "node scripts/migrate.js create",
    "db:list": "node scripts/migrate.js list",
    "db:unlock": "node scripts/migrate.js force-unlock",
    "db:reset": "node scripts/reset-database.js"
  },
  "keywords": [
    "ai",
    "scraping",
    "training-data",
    "e-commerce",
    "automation",
    "machine-learning"
  ],
  "author": "Peter Amadon <pdamadon@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "playwright": "^1.40.0",
    "socket.io": "^4.8.1",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bull": "^3.15.9",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^0.0.48",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.10",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "js-yaml": "^4.1.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Pdamadon/codesight-scraping-ai-generation.git"
  },
  "bugs": {
    "url": "https://github.com/Pdamadon/codesight-scraping-ai-generation/issues"
  },
  "homepage": "https://github.com/Pdamadon/codesight-scraping-ai-generation#readme"
}
