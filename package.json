    "playwright": "^1.40.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "bull": "^4.12.2",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
